{
    "name": "php-graph-group/cypher-query-builder",
    "description": "An opinionated query builder for the Cypher Query language",
    "type": "library",
    "require": {
        "wikibase-solutions/php-cypher-dsl": "^5.0",
        "laudis/neo4j-php-client": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "vimeo/psalm": "^5.11",
        "friendsofphp/php-cs-fixer": "^3.16",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.14"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpGraphGroup\\CypherQueryBuilder\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ghlen Nagels",
            "email": "ghlen@pm.me"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
